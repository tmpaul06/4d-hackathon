{
  "name": "roam-jan-demo",
  "version": "0.0.1",
  "description": "Roam Knowledge Graph demo for JnJ",
  "repository": {
    "type": "git",
    "url": "http://github.com/roaminsight/roam-jan-demo.git"
  },
  "license": "proprietary",
  "scripts": {
    "test": "npm run lint && jest --config=jest-config.js --verbose",
    "start": "node server.js",
    "lint": "eslint --ext .js,.jsx src",
    "prebuild": "rm -rf ./dist && mkdir -p ./dist",
    "build": "npm run prebuild && echo \"built $(date -u)\" > ./dist/version.txt && webpack --config webpack-staging.config.js --progress --colors --profile",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "0.17.0",
    "eslint": "1.2.1",
    "eslint-plugin-react": "3.3.0",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "1.6.1",
    "jest-cli": "0.5.0",
    "json-loader": "0.5.2",
    "less-loader": "2.2.0",
    "react-hot-loader": "1.3.0",
    "react-tools": "0.13",
    "style-loader": "0.12.3",
    "url-loader": "0.5.6",
    "webpack": "1.12.1",
    "webpack-dev-server": "1.10.1"
  },
  "dependencies": {
    "@roam/react-shared": "*",
    "d3": "3.5.9",
    "font-awesome": "4.4.0",
    "history": "^1.13.0",
    "less": "^2.6.1",
    "normalize.css": "3.0.3",
    "react": "0.14.2",
    "react-addons-clone-with-props": "0.14.2",
    "react-color": "^2.1.0",
    "react-dom": "0.14.2",
    "react-input-range": "^0.8.0",
    "react-router": "1.0.0-rc3",
    "scroll-behavior": "0.2.0",
    "superagent": "1.2.0"
  }
}
